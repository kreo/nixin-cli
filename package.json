{
  "name": "nixin-cli",
  "version": "1.3.1",
  "description": "Nixin CLI - A bunch of useful gulp tasks",
  "main": "index.js",
  "scripts": {
    "preinstall": "sudo npm install gulp bower -g",
    "stylus": "stylus --sourcemap-inline resources/styles/index.styl -o public/_dist/app.css",
    "stylus-serve": "stylus --sourcemap-inline -w resources/styles/index.styl -o public/_dist/app.css",
    "browserify": "browserify -d resources/scripts/index.js -o public/_dist/app.js",
    "autoprefixer": "postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions\" -r public/_dist/*.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kreo/nixin-cli.git"
  },
  "keywords": [
    "nix",
    "gulp",
    "global",
    "tasks",
    "stylus",
    "browserify"
  ],
  "author": "@kreo - Fabio Cencetti <hey@kreo.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kreo/nixin-cli/issues"
  },
  "homepage": "https://github.com/kreo/nixin-cli#readme",
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "autoprefixer-stylus": "^0.9.4",
    "browser-sync": "^2.14.0",
    "browserify": "^13.1.0",
    "chalk": "^1.1.3",
    "cssnano": "^3.7.3",
    "del": "^2.2.1",
    "frep": "^0.2.3",
    "fs": "0.0.2",
    "globby": "^6.0.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-browserify": "^0.5.1",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-css-globbing": "^0.1.8",
    "gulp-cssnano": "^2.1.2",
    "gulp-env": "^0.4.0",
    "gulp-flatten": "^0.3.0",
    "gulp-frep": "^0.1.3",
    "gulp-gnirts": "^0.4.5",
    "gulp-group-css-media-queries": "^1.1.0",
    "gulp-gzip": "^1.4.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.2",
    "gulp-inject": "^4.1.0",
    "gulp-inline-css": "^3.1.0",
    "gulp-jade-globbing": "^0.1.9",
    "gulp-js-obfuscator": "^1.0.0",
    "gulp-less": "^3.1.0",
    "gulp-mirror": "^1.0.0",
    "gulp-order": "^1.1.1",
    "gulp-pug": "^3.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.3.2",
    "gulp-shell": "^0.5.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-style-inject": "0.0.3",
    "gulp-stylus": "^2.5.0",
    "gulp-task-listing": "^1.0.1",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "hashmark": "^3.1.0",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.1.0",
    "merge-stream": "^1.0.0",
    "path": "^0.12.7",
    "require-globify": "^1.4.0",
    "run-sequence": "^1.2.2",
    "spritesmith": "^3.1.0",
    "stream": "0.0.2",
    "through2": "^2.0.1",
    "underscore": "^1.8.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-fs": "^2.4.3",
    "vinyl-paths": "^2.1.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^4.8.1"
  },
  "bin": {
    "nix": "./bin/nix"
  },
  "config": {
    "styles": {
      "source": "nixin-cli/source/styles/index.styl",
      "dest": "nixin-cli/_dist/app.css",
      "opt": {}
    },
    "scripts": {
      "source": "nixin-cli/source/scripts/index.js",
      "dest": "nixin-cli/_dist/app.js",
      "opt": {}
    },
    "port": "9001"
  },
  "preferGlobal": true,
  "private": false,
  "publishConfig": {},
  "analyze": true
}
